{
    "materialTheme.fixIconsRunning": false,
    "vim.neovimPath": "/home/chandru/Downloads/nvim.appimage",
    "editor.lineNumbers": "relative",
    "vim.easymotion": true,
    "vim.easymotionJumpToAnywhereRegex": "\\b[A-Za-z0-9]|[A-Za-z0-9]\\b|_.|#.|[a-z][A-Z]",
    "vim.easymotionKeys": "hklyuiopnm,qwertzxcvbasdgjf;",
    "vim.hlsearch": true,
    "vim.incsearch": true,
    "vim.leader": ",",
    "vim.showcmd": true,
    "vim.visualstar": true,
    "window.zoomLevel": -1,
    "workbench.iconTheme": "Monokai Classic Icons",
    "vim.enableNeovim": true,
    "vim.sneakUseIgnorecaseAndSmartcase": true,
    "editor.formatOnSave": true,
    "editor.formatOnPaste": true,
    "go.autocompleteUnimportedPackages": true,
    "go.lintTool": "gometalinter",
    "vim.mouseSelectionGoesIntoVisualMode": false,
    "vim.useSystemClipboard": true,
    "search.location": "panel",
    // "vim.easymotionMarkerBackgroundColor": "white",
    // "vim.easymotionMarkerForegroundColorOneChar": "black",
    "editor.minimap.enabled": false,
    "diffEditor.ignoreTrimWhitespace": false,
    "workbench.activityBar.visible": true,
    "workbench.editor.enablePreview": false,
    "vim.insertModeKeyBindings": [
        {
            "before": [
                "j",
                "j"
            ],
            "after": [
                "<Esc>"
            ]
        }
    ],
    "vim.normalModeKeyBindingsNonRecursive": [
        {
            "before": [
                "<leader>",
                ":"
            ],
            "commands": [
                "workbench.action.showCommands"
            ]
        }
    ],
    "vim.useCtrlKeys": false,
    "editor.tabCompletion": "on",
    "go.useCodeSnippetsOnFunctionSuggest": true,
    "go.useCodeSnippetsOnFunctionSuggestWithoutType": true,
    "go.coverOnTestPackage": false,
    "go.docsTool": "guru",
    "go.installDependenciesWhenBuilding": false,
    // "go.vetFlags": [
    //     "all"
    // ],
    "editor.useTabStops": false,
    "go.vetOnSave": "off",
    "editor.formatOnType": true,
    "vim.camelCaseMotion.enable": true,
    "vim.sneak": true,
    "workbench.sideBar.location": "left",
    "git.autofetch": true
}
